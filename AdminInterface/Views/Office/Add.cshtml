@model Repositories.Models.Admin.Office.NewOffice
@{
    ViewBag.Title = "Add New Office Here";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Add New Office Here</h2>
<section class="content">
    <div class="box box-default">
        <div class="box-header with-border">

            <div class="form-group pull-right col-lg-1">
                <input type="button" class="btn btn-primary btn-block " value="Back" onclick="window.history.back();" data-loading-text="Please wait..." />
            </div>
            @*<div class="box-tools pull-right">
                    <button class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                </div>*@
        </div><!-- /.box-header -->
        <div class="box-body">
            <div class="row">
                <div class="col-md-12">
                    <div id="sucessalert" class="alert alert-success alert-dismissible" style="display: none">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                        <h4><i class="icon fa fa-check"></i> Office Added Successfully.</h4>
                        <span></span>
                    </div>
                    <div id="sucessupdatealert" class="alert alert-success alert-dismissible" style="display: none">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                        <h4><i class="icon fa fa-check"></i> Office Updated Successfully.</h4>
                        <span></span>
                    </div>
                    <div id="failurealert" class="alert alert-danger alert-dismissible" style="display: none">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                        <h4><i class="icon fa fa-ban"></i> Something went wrong! Please try again later</h4>
                        <span></span>
                    </div>
                    <div id="validationalert" class="alert alert-danger alert-dismissible" style="display: none">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                        <h4><i class="icon fa fa-ban"></i><label id="lblMsg"></label> </h4>
                        <span></span>
                    </div>
                </div>
                @Html.HiddenFor(m => m.OfficeId)
                @Html.HiddenFor(m => m.OfficeCode.OfficeCodeId)
                @Html.HiddenFor(m => m.OfficeKey)
                @Html.HiddenFor(m => m.MainOfficeId)
                @Html.HiddenFor(m => m.OfficeLogo)
            </div><!-- /.row -->
            <div class="row">

                <div class="col-md-6">

                    <div class="form-group">
                        @Html.LabelFor(m => m.CorporateName)
                        @Html.TextBoxFor(m => m.CorporateName, new { @class = "form-control" })

                    </div>
                </div><!-- /.col -->
                <div class="col-md-6">

                    <div class="form-group">
                        @Html.Label("Name")

                        @Html.TextBoxFor(m => m.Name, new { @class = "form-control" })

                    </div>
                </div><!-- /.col -->
            </div><!-- /.row -->

            <div class="row">
                <div class="col-md-6">

                    <div class="form-group">
                        @Html.LabelFor(m => m.MainOfficeId)
                        @Html.TextBoxFor(m => m.MainOfficeId, new { @class = "form-control" })

                    </div>
                </div><!-- /.col -->

                <div class="col-md-6">

                    <div class="form-group">
                        @Html.Label("Broker Id")

                        @Html.TextBoxFor(m => m.BrokerId, new { @class = "form-control" })

                    </div>
                </div><!-- /.col -->
            </div>

            <div class="row">


                <div class="col-md-6">

                    <div class="form-group">
                        @Html.Label("Phone Number")

                        @Html.TextBoxFor(m => m.PhoneNumber, new { @class = "form-control" })

                    </div>
                </div><!-- /.col -->
                <div class="col-md-6">

                    <div class="form-group">
                        @Html.LabelFor(m => m.Address.StateOrProvince)
                        @Html.DropDownListFor(M => M.Address.StateOrProvince, new SelectList(Model.StateList, "StateOrProvince", "StateOrProvince"), "--All State--", new { @class = "form-control" })
                        <input type="text" id="Other_StateOrProvince" class="form-control" style="display: none" />
                    </div>
                </div><!-- /.col -->
            </div>

            <div class="row">


                <div class="col-md-6">

                    <div class="form-group">
                        @Html.LabelFor(m => m.Address.City)
                        @Html.DropDownListFor(M => M.Address.City, new SelectList(Model.CityList, "City", "City"), "--All City--", new { @class = "form-control" })
                        <input type="text" id="Other_City" class="form-control" style="display: none" />
                    </div>
                </div><!-- /.col -->
                <div class="col-md-6">

                    <div class="form-group">
                        @Html.LabelFor(m => m.Address.PostalCode)
                        @Html.DropDownListFor(M => M.Address.PostalCode, new SelectList(Model.ZipCodeList, "PostalCode", "PostalCode"), "--All PostalCode--", new { @class = "form-control" })
                        <input type="text" id="Other_PostalCode" class="form-control" style="display: none" />
                    </div>
                </div><!-- /.col -->
            </div>

            <div class="row">

                <div class="col-md-6">

                    <div class="form-group">
                        @Html.LabelFor(m => m.Address.FullStreetAddress)
                        @Html.DropDownListFor(M => M.Address.FullStreetAddress, new SelectList(Model.StreetList, "FullStreetAddress", "FullStreetAddress"), "--All StreetAddress--", new { @class = "form-control" })
                        <input type="text" id="Other_FullStreetAddress" class="form-control" style="display: none" />
                    </div>
                </div><!-- /.col -->

                <div class="col-md-6">

                    <div class="form-group">
                        @Html.LabelFor(m => m.Website)
                        @Html.TextBoxFor(m => m.Website, new { @class = "form-control" })

                    </div>
                </div><!-- /.col -->
            </div>

            <div class="row">
                <div class="col-md-6">

                    <div class="form-group">
                        @Html.LabelFor(m => m.OfficeEmail)
                        @Html.TextBoxFor(m => m.OfficeEmail, new { @class = "form-control" })

                    </div>
                </div><!-- /.col -->
                <div class="col-md-6">
                    <div class="form-group">
                        @Html.Label("Description")
                        @Html.TextBoxFor(m => m.OfficeDescription, new { @class = "form-control" })
                    </div>
                </div><!-- /.col -->
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="form-group">
                        <label>Logo Image </label>
                        <form id="fileuploadLogo" action="/Upload/UploadHandler.ashx" method="POST" enctype="multipart/form-data">
                            <!-- SELECT3 IMAGE UPLOAD -->

                            <div class="box-body">
                                <div class="row">
                                    <div class="col-md-6">
                                        <!-- The fileupload-buttonbar contains buttons to add/delete files and start/cancel the upload -->
                                        <div class="fileupload-buttonbar">
                                            <div class="span7">
                                                <!-- The fileinput-button span is used to style the file input field as button -->
                                                <span class="btn btn-success fileinput-button">
                                                    <i class="icon-plus icon-white"></i>
                                                    <span>Add files...</span>
                                                    <input type="file" name="files[]" multiple>
                                                </span>
                                            </div>
                                            <div class="span5">
                                                <!-- The global progress bar -->
                                                <div class="progress progress-success progress-striped active fade">
                                                    <div class="bar" style="width:0%;"></div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- The loading indicator is shown during image processing -->
                                        <div class="fileupload-loading"></div>
                                        <br>
                                        <!-- The table listing the files available for upload/download -->
                                        <table class="table table-striped" id="tblImage"><tbody class="files" data-toggle="modal-gallery" data-target="#modal-gallery"></tbody></table>

                                    </div><!-- /.col -->
                                </div><!-- /.row -->
                                @*<div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group pull-right">
                                                <input id="btnImageUpload" type="button" data-uniqueid='@Model.CommunityId' data-type='Communities' class="btn btn-primary btn-block " value="Update" />
                                            </div><!-- /.form-group -->
                                        </div><!-- /.col -->
                                    </div>*@
                            </div><!-- /.box-body -->
                            <!-- /.box -->
                        </form>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="form-group pull-right">
                        @if (String.IsNullOrEmpty(Model.OfficeId))
                        {
                            <input type="button" class="btn btn-primary btn-block " value="Save" onclick="AddOffice(this)" data-loading-text="Please wait..." />
                        }
                        else
                        {

                            <input type="button" class="btn btn-primary btn-block " value="Save" onclick="EditOffice(this)" data-loading-text="Please wait..." />
                        }
                    </div><!-- /.form-group -->
                </div><!-- /.col -->
            </div>
        </div><!-- /.box-body -->
    </div>

    <div class="box box-default hide" id="UploadImageBox">
        <div class="box-header with-border">
            <h3 class="box-title">Upload office Image</h3>
            <div class="box-tools pull-right">
                <button class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
            </div>
        </div><!-- /.box-header -->
        <div class="box-body">

            <div class="row">
                <div class="col-lg-12">
                    <div id="sucessUploadImagealert" class="alert alert-success alert-dismissible" style="display: none">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                        <h4><i class="icon fa fa-check"></i> Image Uploaded Successfully.</h4>
                        <span></span>
                    </div>
                </div>
                <div class="col-md-8">
                    <label class="pull-left">Upload office image here </label>
                </div>
                <div class="col-md-4">
                    <form id="fileupload" action="/Upload/OfficeImageUpload.ashx" method="POST" enctype="multipart/form-data">
                        <div class="fileupload-buttonbar">
                            <div class="span7">
                                <span class="btn btn-success fileinput-button">
                                    <i class="icon-plus icon-white"></i>
                                    <span>Add file</span>
                                    <input type="file" name="files[]">
                                </span>
                                <button type="button" id="btnImageUpload" class="btn btn-primary start" data-uniqueid='@Model.OfficeId' data-type='newhome'>
                                    <i class="fa fa-cloud-upload"></i>
                                </button>
                            </div>
                        </div>
                        <table class="table table-striped"><tbody class="files" data-toggle="modal-gallery" data-target="#modal-gallery"></tbody></table>
                        @*<a data-toggle="tooltip" data-placement="top" title="Click here to send activation mail." id="btnactive_" class="btn btn-block btn-primary pull-right" href="javascript:SendActivationMail();">Upload</a>*@
                    </form>

                </div>

            </div><!-- /.col -->
        </div>
    </div>

</section>
@section css
