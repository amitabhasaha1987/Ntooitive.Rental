    <script id="template-upload" type="text/x-tmpl">
        {% for (var i=0, file; file=o.files[i]; i++) { %}
        <tr class="template-upload fade">
            <td class="preview"><span class="fade"></span></td>
            <td class="name"><span>{%=file.name%}</span></td>
            <td class="size"><span>{%=o.formatFileSize(file.size)%}</span></td>
            {% if (file.error) { %}
            <td class="error" colspan="2"><span class="label label-important">{%=locale.fileupload.error%}</span> {%=locale.fileupload.errors[file.error] || file.error%}</td>
            {% } else if (o.files.valid && !i) { %}
            <td>
                <div class="progress progress-success progress-striped active"><div class="bar" style="width:0%;"></div></div>
            </td>
            <td class="start">
                {% if (!o.options.autoUpload) { %}
                <button class="btn btn-primary">
                    <i class="icon-upload icon-white"></i>
                    <span>{%=locale.fileupload.start%}</span>
                </button>
                {% } %}
            </td>
            {% } else { %}
            <td colspan="2"></td>
            {% } %}
            <td class="cancel">
                {% if (!i) { %}
                <button class="btn btn-warning">
                    <i class="icon-ban-circle icon-white"></i>
                    <span>{%=locale.fileupload.cancel%}</span>
                </button>
                {% } %}
            </td>
        </tr>
        {% } %}
    </script>
    <!-- The template to display files available for download -->
    <script id="template-download" type="text/x-tmpl">
        {% for (var i=0, file; file=o.files[i]; i++) { %}
        <tr class="template-download fade">
            {% if (file.error) { %}
            <td></td>
            <td class="name"><span>{%=file.name%}</span></td>
            <td class="size"><span>{%=o.formatFileSize(file.size)%}</span></td>
            <td class="error" colspan="2"><span class="label label-important">{%=locale.fileupload.error%}</span> {%=locale.fileupload.errors[file.error] || file.error%}</td>
            {% } else { %}
            <td class="preview">
                {% if (file.thumbnail_url) { %}
                @*<span>{%=file.name%}</span>*@
                <img src="{%=file.thumbnail_url%}">
                {% } %}
            </td>
            <td class="name">
                <span>{%=file.name%}</span>
            </td>
            @*<td class="size"><span>{%=o.formatFileSize(file.size)%}</span></td>*@
            <td colspan="2"></td>
            {% } %}
            <td class="delete">
                <button class="btn btn-danger" data-type="{%=file.delete_type%}" data-url="{%=file.delete_url%}" type="button">
                    <i class="icon-trash icon-white"></i>
                    <span>{%=locale.fileupload.destroy%}</span>
                </button>
                <input type="checkbox" name="delete" value="1">
            </td>
        </tr>
        {% } %}

    </script>

    <script id="template-upload" type="text/x-tmpl">
        {% for (var i=0, file; file=o.files[i]; i++) { %}
        <tr class="template-upload fade">
            <td class="preview"><span class="fade"></span></td>
            <td class="name"><span>{%=file.name%}</span></td>
            {% if (file.error) { %}
            <td class="error" colspan="2"><span class="label label-important">{%=locale.fileupload.error%}</span> {%=locale.fileupload.errors[file.error] || file.error%}</td>
            {% } else if (o.files.valid && !i) { %}

            <td class="start">
                {% if (!o.options.autoUpload) { %}
                <button style="display:none" class="btn btn-primary">
                    <i class="icon-upload icon-white"></i>
                    <span>{%=locale.fileupload.start%}</span>
                </button>
                {% } %}
            </td>
            {% } else { %}
            <td colspan="2"></td>
            {% } %}

        </tr>
        {% } %}
    </script>
    <script id="template-download" type="text/x-tmpl">
        Uploaded successfully!
    </script>
    <script>

        function BackListing(btn) {
            window.location.href = "/office/office-listing?liId=officeList";
        }

        function AddOffice(btn) {
            debugger;
            var Name = $("#Name").val();
            var CorporateName = $("#CorporateName").val();
            var BrokerId = $("#BrokerId").val();
            var MainOfficeId = $("#MainOfficeId").val();
            var PhoneNumber = $("#PhoneNumber").val();
            var State = $("#Address_StateOrProvince").val();
            var City = $("#Address_City").val();
            var FullStreetAddress = $("#Address_FullStreetAddress").val();
            var PostalCode = $("#Address_PostalCode").val();
            var OfficeEmail = $("#OfficeEmail").val();
            var Website = $("#Website").val();
            var OfficeDescription = $("#OfficeDescription").val();
            debugger;
            var imageLst = new Array();
            $('#tblImage .name ').each(function () {
                var a = $(this).find("span").html();
                var photo = {};
                photo.MediaURL = a;
                imageLst.push(photo);
            });

            if (State == "Other") {
                State = $("#Other_StateOrProvince").val();
            }
            if (City == "Other") {
                City = $("#Other_City").val();
            }
            if (FullStreetAddress == "Other") {
                FullStreetAddress = $("#Other_FullStreetAddress").val();
            }
            if (PostalCode == "Other") {
                PostalCode = $("#Other_PostalCode").val();
            }

            var mailformat = /^\w+([\.-]?\w+)*@@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
            var numbers = /^[0-9]+$/;


            if (CorporateName == "") {
                $("#validationalert").show();
                $('#validationalert').delay(3000).fadeOut(400);
                $("#lblMsg").html("Corporate Name Cannot be null");
                $("#CorporateName").focus();
            } else if (Name == "") {
                $("#validationalert").show();
                $('#validationalert').delay(3000).fadeOut(400);
                $("#lblMsg").html("Name Cannot be null");
                $("#Name").focus();
            } else if (PhoneNumber == "") {
                $("#validationalert").show();
                $('#validationalert').delay(3000).fadeOut(400);
                $("#lblMsg").html("Phone Number Cannot be null");
                $("#PhoneNumber").focus();
            } else if (!numbers.test(PhoneNumber)) {
                $("#validationalert").show();
                $('#validationalert').delay(3000).fadeOut(400);
                $("#lblMsg").html("Please enter valid phone no.");
                $("#PhoneNumber").focus();
            } else if (State == "") {
                $("#validationalert").show();
                $('#validationalert').delay(3000).fadeOut(400);
                $("#lblMsg").html("Please Select State");
                $("#Address_StateOrProvince").focus();
            } else if (City == "" || City == "0") {
                $("#validationalert").show();
                $('#validationalert').delay(3000).fadeOut(400);
                $("#lblMsg").html("Please Select City");
                $("#Address_City").focus();
            } else if (PostalCode == "" || PostalCode == "0") {
                $("#validationalert").show();
                $('#validationalert').delay(3000).fadeOut(400);
                $("#lblMsg").html("Please Select Postal Code");
                $("#Address_PostalCode").focus();
            } else if (FullStreetAddress == "" || FullStreetAddress == "0") {
                $("#validationalert").show();
                $('#validationalert').delay(3000).fadeOut(400);
                $("#lblMsg").html("Please Select Street Address");
                $("#Address_FullStreetAddress").focus();
            } else if (OfficeEmail == "") {
                $("#validationalert").show();
                $('#validationalert').delay(3000).fadeOut(400);
                $("#lblMsg").html("Email Cannot be null");
                $("#OfficeEmail").focus();
            } else if (!mailformat.test(OfficeEmail)) {
                $("#validationalert").show();
                $('#validationalert').delay(3000).fadeOut(400);
                $("#lblMsg").html("Please enter valid email.");
                $("#OfficeEmail").focus();
            } else if (Website == "") {
                $("#validationalert").show();
                $('#validationalert').delay(3000).fadeOut(400);
                $("#lblMsg").html("Website Cannot be null");
            } else {

                var formData = {
                    "Name": Name,
                    "CorporateName": CorporateName,
                    "BrokerId": BrokerId,
                    "MainOfficeId": MainOfficeId,
                    "PhoneNumber": PhoneNumber,
                    "Address.StateOrProvince": State,
                    "Address.City": City,
                    "Address.FullStreetAddress": FullStreetAddress,
                    "Address.PostalCode": PostalCode,
                    "OfficeEmail": OfficeEmail,
                    "Website": Website,
                    "OfficeDescription": OfficeDescription,
                    "OfficeLogo": ''
                };
                if (imageLst.length > 0) {
                    formData.OfficeLogo = imageLst[0].MediaURL;
                }
                $(btn).button('loading').queue(function () {
                    $.ajax({
                        url: "/office/add-office",
                        type: "POST",
                        contentType: 'application/json',
                        dataType: "json",
                        data: JSON.stringify(formData),
                        success: function (data, textStatus, jqXHR) {
                            if (data.success) {
                                $("#sucessalert").show();
                                $('#sucessalert').delay(5000).fadeOut(400);
                                //$('#UploadImageBox').removeClass('hide');
                                $('#fileupload').fileupload();

                                $('#fileupload').fileupload('option', {
                                    maxFileSize: 500000000,
                                    resizeMaxWidth: 1920,
                                    resizeMaxHeight: 1200,
                                    formData: { OfficeId: '@Model.OfficeId' }

                                });
                            } else {
                                $("#failurealert").show();
                                $('#failurealert').delay(5000).fadeOut(400);
                            }
                        },
                        error: function (jqXHR, textStatus, errorThrown) {
                            $("#failurealert").show();
                            $('#failurealert').delay(5000).fadeOut(400);
                        },
                        compete: function () {
                            $(btn).button('reset');
                            $(btn).dequeue();
                        }
                    });
                });
            }

        }

