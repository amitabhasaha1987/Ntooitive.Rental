            </div><!-- /.tab-pane -->

        </div>
    </div><!-- nav-tabs-custom -->


</section><!-- /.content -->
@section scripts
{
    <!-- DataTables -->
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.18/jquery-ui.min.js?cdv=1"></script>
    <script src="~/Content/CustomJs.js"></script>

    <script src="~/Content/FileuploaderJs/tmpl.min.js"></script>
