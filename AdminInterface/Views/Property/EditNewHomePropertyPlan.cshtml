    <script type="text/javascript">
        $(function () {
            'use strict';
            $('#fileupload').fileupload();

            $('#fileupload').fileupload('option', {
                maxFileSize: 500000000,
                resizeMaxWidth: 1920,
                resizeMaxHeight: 1200,
                formData: { example: '@Model.BuilderNumber' }

            });
        });
    </script>

    <script src="~/Scripts/custom/moment.min.js"></script>
