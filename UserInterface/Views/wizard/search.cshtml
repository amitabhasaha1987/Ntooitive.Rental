<script type="text/javascript">
    $(function () {
        $('#allClick').click(function (e) {
            var name1 = $("#txtAll").val();
            var name;
            if (name1.indexOf("#") > -1) {
                name = name1.substring(1, name1.length);
                var url = url_Start + '@Url.Action("Index", "Index/PropertiesListing")' + '?searchTerm=' + name + '&Command=All&IsMls=true&SubCommand=Purchase&SubTab=Newest';
                window.location.href = url;
            } else {
                name = name1;
                var url = url_Start + '@Url.Action("Index", "Index/PropertiesListing")' + '?searchTerm=' + name + '&Command=All&IsMls=false&SubCommand=Purchase&SubTab=Newest';
                window.location.href = url;
            }
        });
    });
</script>

