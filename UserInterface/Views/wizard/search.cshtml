<style>
    .ui-autocomplete {
        max-height: 150px;
        position: absolute;
        overflow-y: auto;
        padding: 10px;
        z-index: 999;
        background-color: #fff;
        /* prevent horizontal scrollbar */
        overflow-x: hidden;
        /* add padding to account for vertical scrollbar */
    }


    li a::after {
        color: red !important;
    }


    html .ui-autocomplete {
        height: auto;
    }

    li a:hover {
        color: black !important;
        transition: color 0.5s linear; /* vendorless fallback */
        -o-transition: color 0.5s linear; /* opera */
        -ms-transition: color 0.5s linear; /* IE 10 */
        -moz-transition: color 0.5s linear; /* Firefox */
        -webkit-transition: color 0.5s linear; /*safari and chrome */
    }
</style>

<script type="text/javascript">

    function show_visibility(id) {
        var e = document.getElementById(id);

        e.style.display = 'block';

    }
    function hide_visibility(id) {
        var e = document.getElementById(id);

        e.style.display = 'none';
    }

    function showExample(divid, exampleId) {
        var id = "#" + divid;
        $(id).focus(function () {
            show_visibility(exampleId);
        });

        $(id).keypress(function () {
            var currentString = $(id).val();
            //alert(currentString.length);
            if (currentString.length >= 0) {  /*or whatever your number is*/
                hide_visibility(exampleId);
            } else {
                show_visibility(exampleId);
            }
        });
    }


    debugger;
    function loadAutoComplete() {
        $("#txtSearchPurchase").autocomplete({
            source: url_Start + '@Url.Action("GetAutoCompleteDataForPurchase", "Index")'
        });

        $("#txtSearchRent").autocomplete({
            source: url_Start + '@Url.Action("GetAutoCompleteDataForRent", "Index")'
        });

        $("#txtSearchNew").autocomplete({
            source: url_Start + '@Url.Action("GetAutoCompleteDataForHome", "Index")'
        });

        $("#txtSearchopenHouse").autocomplete({
            source: url_Start + '@Url.Action("GetAutoCompleteDataForAll", "Index")'
        });
    }

    $(function () {

        showExample('txtSearchPurchase', 'purchase_example');
        showExample('txtSearchRent', 'rent_example');
        showExample('txtSearchNew', 'new-home_example');
        showExample('txtSearchopenHouse', 'open-house_example');


        $("#txtSearchPurchase").blur(function () {
            $("#purchase_example").hide();
        });
        $("#txtSearchRent").blur(function () {
            $("#rent_example").hide();
        });
        $("#txtSearchNew").blur(function () {
            $("#new-home_example").hide();
        });
        $("#txtSearchopenHouse").blur(function () {
            $("#open-house_example").hide();
        });

        var jquery_ui_js = url_Start + '/js/jquery-ui-1.10.4.custom.min.js';
        loadScript(jquery_ui_js, function () {loadAutoComplete(); });
    });


</script>

