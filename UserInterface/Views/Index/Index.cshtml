@model IEnumerable<Repositories.Models.ListHub.ListHubListing>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script type="text/javascript">
    $(function () {
        $('#txtSearchPurchase').keypress(function (e) {

            if (e.which == 13) {
                $('#purchaseClick').click();
                return false;    //<---- Add this line
            }
        });

        $('#txtAll').keypress(function (e) {

            if (e.which == 13) {
                $('#allClick').click();
                return false;    //<---- Add this line
            }
        });

        $('#txtSearchRent').keypress(function (e) {

            if (e.which == 13) {
                $('#rentClick').click();
                return false;    //<---- Add this line
            }
        });

        $('#txtSearchNew').keypress(function (e) {

            if (e.which == 13) {
                $('#newHomeClick').click();
                return false;    //<---- Add this line
            }
        });

        $('#txtSearchopenHouse').keypress(function (e) {

            if (e.which == 13) {
                $('#openHouseClick').click();
                return false;    //<---- Add this line
            }
        });

        $('#txtAgent').keypress(function (e) {

            if (e.which == 13) {
                $('#openAgentClick').click();
                return false;    //<---- Add this line
            }
        });
    });
</script>

<script type="text/javascript">
    $(function () {
        $('#allClick').click(function (e) {
            var name1 = $("#txtAll").val();
            var name;
            if (name1.indexOf("#") > -1) {
                name = name1.substring(1, name1.length);
                var url = '@Url.Action("Index", "Index/PropertiesListing")' + '?searchTerm=' + name + '&Command=All&IsMls=true&SubCommand=Purchase&SubTab=Newest' + '&ViewStyle=PropertiesListing&loadCount=1';
                window.location.href = url;
            } else {
                name = name1;
                var url = '@Url.Action("Index", "Index/PropertiesListing")' + '?searchTerm=' + name + '&Command=All&IsMls=false&SubCommand=Purchase&SubTab=Newest' + '&ViewStyle=PropertiesListing&loadCount=1';
                window.location.href = url;
            }
        });
    });
</script>

